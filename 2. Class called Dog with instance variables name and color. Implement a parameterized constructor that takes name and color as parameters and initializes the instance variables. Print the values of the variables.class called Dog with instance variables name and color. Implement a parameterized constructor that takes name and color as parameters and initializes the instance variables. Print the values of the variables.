public class Dog {
    String name;
    String color;

    public Dog(String name, String color) {
        this.name = name;
        this.color = color;
    }

    public static void main(String[] args) {
        Dog myDog = new Dog("Buddy", "Brown");
        System.out.println("Name: " + myDog.name);
        System.out.println("Color: " + myDog.color);
    }
}
